{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/src/main.ts",
      "methods": ["POST","GET","PUT","DELETE","PATCH","OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "192.168.45.123"
      }
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "builds": [
    {
      "src": "server/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    }
  ]
}
