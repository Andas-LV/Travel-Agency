{
  "version": 2,
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "builds": [
    {
      "src": "server/main.go",
      "use": "@vercel/go",
      "config": {"distDir": "api"}
    },
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    }
  ]
}
